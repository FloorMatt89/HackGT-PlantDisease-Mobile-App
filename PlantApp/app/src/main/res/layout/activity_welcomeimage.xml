<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="welcomeimageVM" type="com.matthewsapplication.app.modules.welcomeimage.data.viewmodel.WelcomeimageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnbranchtop"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearWelcomeImage"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <ImageView android:id="@+id/imageBranchTop"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_216pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_375pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_branchtop}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_branchtop"/>
            <LinearLayout android:id="@+id/linearColumnimgleaflogo"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowimgleaflogo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImgleafLogo"
                        android:layout_height="@dimen/_159pxh"
                        android:layout_marginBottom="@dimen/_52pxv"
                        android:layout_width="@dimen/_133pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_imgleaflogo}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_imgleaflogo"/>
                    <TextView android:id="@+id/txtLogoText"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_41pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp"
                        android:text="@{welcomeimageVM.welcomeimageModel.txtLogoText}"
                        style="@style/txtUrbanistromanbold70" tools:text="@string/lbl_lant_md"/>
                </LinearLayout>
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackbranchbottom"
                    android:layout_height="@dimen/_162pxv"
                    android:layout_marginTop="@dimen/_189pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBranchBottom"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_162pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_branchbottom}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_branchbottom"/>
                    <LinearLayout
                        android:id="@+id/linearColumnslideonethree"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_31pxh"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearPositionOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_47pxh"
                            android:layout_marginStart="@dimen/_57pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageSlideOneThree"
                                android:layout_height="@dimen/_29pxh"
                                android:layout_width="@dimen/_29pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_slideonethree"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_slideonethree"/>
                            <ImageView
                                android:id="@+id/imageSlideOneTwo"
                                android:layout_height="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_12pxh"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_slideonetwo"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_slideonetwo"/>
                            <ImageView
                                android:id="@+id/imageSlideOneOne"
                                android:layout_height="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_slideonetwo"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_slideonetwo"/>
                            <ImageView
                                android:id="@+id/imageSlideOneFive"
                                android:layout_height="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_slideonetwo"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_slideonetwo"/>
                            <ImageView
                                android:id="@+id/imageSlideOneFour"
                                android:layout_height="@dimen/_29pxh"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_width="@dimen/_25pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_slideonetwo"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_slideonetwo"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnNext"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_15pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_15pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_next"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded" tools:text="@string/lbl_next"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
